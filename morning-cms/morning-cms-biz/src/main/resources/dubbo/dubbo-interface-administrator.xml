<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd
        http://code.alibabatech.com/schema/dubbo
        http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

	<!-- 服务提供者暴露服务配置：服务接口名及服务对象实现引用 -->
	<dubbo:service
		interface="com.pussinboots.morning.cms.modules.administrator.service.IRoleMenuService"
		ref="roleMenuServiceImpl" />
	<dubbo:service
		interface="com.pussinboots.morning.cms.modules.administrator.service.IRoleService"
		ref="roleServiceImpl" />
	<dubbo:service
		interface="com.pussinboots.morning.cms.modules.administrator.service.IUserLoginLogService"
		ref="userLoginLogServiceImpl" />
	<dubbo:service
		interface="com.pussinboots.morning.cms.modules.administrator.service.IUserRoleService"
		ref="userRoleServiceImpl" />
	<dubbo:service
		interface="com.pussinboots.morning.cms.modules.administrator.service.IUserService"
		ref="userServiceImpl" />
	<dubbo:service
		interface="com.pussinboots.morning.cms.modules.administrator.service.IOrganizationService"
		ref="organizationServiceImpl" />		
		

</beans>